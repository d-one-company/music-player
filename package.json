{
  "name": "music-player",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "v20"
  },
  "scripts": {
    "dev": "cross-env NODE_NO_WARNINGS=1 next dev --turbo",
    "build": "cross-env NODE_NO_WARNINGS=1 next build",
    "start": "cross-env NODE_NO_WARNINGS=1 next start",
    "lint": "pnpm run lint:js",
    "lint:css": "stylelint \"**/*.css\" --allow-empty-input --cache --cache-strategy=content --cache-location=.stylelintcache",
    "lint:js": "eslint \"**/*.{js,mjs,ts,tsx}\" --cache --cache-strategy=content --cache-location=.eslintjscache",
    "lint:fix": "pnpm run lint:js --fix",
    "prettier": "prettier \"**/*.{js,mjs,ts,tsx,md,mdx,json,yml,css}\" --check --cache --cache-strategy=content --cache-location=.prettiercache",
    "prettier:fix": "pnpm run prettier --write",
    "format": "pnpm run lint:fix && pnpm run prettier:fix",
    "prepare": "husky"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@t3-oss/env-nextjs": "^0.9.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cross-env": "^7.0.3",
    "husky": "^9.0.11",
    "jiti": "^1.21.0",
    "lint-staged": "^15.2.2",
    "lucide-react": "^0.368.0",
    "next": "14.2.1",
    "next-auth": "^4.24.7",
    "prettier": "^3.2.5",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.1",
    "eslint-config-prettier": "^9.1.0",
    "postcss": "^8",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "stylelint": "^16.3.1",
    "tailwindcss": "^3.4.1",
    "turbo": "^1.13.2",
    "typescript": "^5"
  }
}
